{
  "name": "onecrm-backend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.883.0"
  },
  "devDependencies": {
    "c8": "^10.1.3"
  },
  "scripts": {
    "start": "node src/server.js",
    "dev": "node --watch src/server.js",
    "check:compliance": "node scripts/check-compliance.mjs",
    "test": "node --test",
    "test:coverage": "c8 --reporter=text-summary --check-coverage --lines 90 --branches 70 node --test",
    "verify": "npm run check:compliance && npm run test:coverage"
  }
}
